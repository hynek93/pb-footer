<div class="middle aligned column">
    <div class="ui right floated buttons">
        {% if definition.actionGroups.main is defined %}
            {% for action in definition.getEnabledActions('main') %}
                {{ sylius_grid_render_action(resources, action, null) }}
            {% endfor %}
        {% endif %}

        <a class="ui labeled icon button  default " href="{{ path('praguebest_sylius_footer_plugin_admin_footer_index') }}">
            <i class="icon zoom"></i>
            {{ 'praguebest_sylius_footer_plugin.ui.back_to_footer'|trans }}
        </a>
    </div>
</div>
